{
  "name": "catch-frontend-ng20",
  "version": "3.1.0",
  "scripts": {
    "___Local Development___": "",
    "dev": "concurrently -r -k -n NG,API -c cyan,magenta \"npm:local:ng\" \"npm:local:api\"",
    "local:ng": "ng serve --port 4000 --configuration local",
    "local:api": "tsx watch src/server/local.api.ts",
    "___Stage Builds___": "",
    "build:stage": "ng build --configuration stage-deployment --stats-json --source-map",
    "___Production Builds___": "",
    "watch": "ng build --watch --configuration prod-deployment",
    "build": "ng build --configuration prod-deployment --stats-json --source-map",
    "build:prod": "npm run build",
    "build:docker": "ng build --configuration prod-deployment --output-hashing=all --stats-json=false --source-map=false",
    "stats": "npx esbuild-visualizer --metadata ./dist/catch-frontend-ng20/stats.json --filename ./dist/catch-frontend-ng20/stats.html",
    "___Serving Builds___": "",
    "serve:ssr:catch-frontend-ng20": "node dist/catch-frontend-ng20/server/server.mjs",
    "serve:ssr": "npm run serve:ssr:catch-frontend-ng20",
    "serve": "npm run serve:ssr",
    "___Redundant___": "",
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.2.0",
    "@angular/cdk": "^20.2.0",
    "@angular/common": "^20.2.0",
    "@angular/compiler": "^20.2.0",
    "@angular/core": "^20.2.0",
    "@angular/forms": "^20.2.0",
    "@angular/material": "^20.2.0",
    "@angular/platform-browser": "^20.2.0",
    "@angular/platform-server": "^20.2.0",
    "@angular/router": "^20.2.0",
    "@angular/ssr": "^20.2.1",
    "@aws-sdk/client-s3": "^3.892.0",
    "@json2csv/plainjs": "^7.0.6",
    "@ngrx/effects": "^20.0.1",
    "@ngrx/router-store": "^20.0.1",
    "@ngrx/store": "^20.0.1",
    "angular-plotly.js": "^20.0.0",
    "dotenv": "^16.4.5",
    "exifreader": "^4.31.2",
    "express": "^5.1.0",
    "file-saver": "^2.0.5",
    "geolib": "^3.3.4",
    "jose": "^5.9.6",
    "jszip": "^3.10.1",
    "plotly.js-dist-min": "^3.1.0",
    "rxjs": "~7.8.0",
    "sbn-solar-viewer": "^0.4.5",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.2.1",
    "@angular/cli": "^20.2.1",
    "@angular/compiler-cli": "^20.2.0",
    "@types/express": "^5.0.1",
    "@types/file-saver": "^2.0.7",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.17.19",
    "@types/plotly.js-dist-min": "^2.3.4",
    "concurrently": "^9.1.2",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "source-map-explorer": "^2.5.3",
    "three": "^0.128.0",
    "tsx": "^4.19.2",
    "typescript": "~5.9.2",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
