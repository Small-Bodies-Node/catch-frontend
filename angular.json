{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "cli": {
    "packageManager": "npm",
    "analytics": false
  },
  "newProjectRoot": "projects",
  "projects": {
    "catch-frontend-ng20": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "browser": "src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              },
              "src/assets",
              "src/assets/fits",
              "src/assets/js9-to-be-served",
              "src/assets/js9-to-be-compiled",
              "src/assets/socket.io",
              {
                "glob": "**/*",
                "input": "src/assets/socket.io/",
                "output": "/socket.io/"
              }
            ],
            "styles": ["src/styles/styles.scss"],
            "scripts": [
              "src/assets/js9-to-be-compiled/js9prefs.js",
              "src/assets/js9-to-be-compiled/js9support.min.js",
              "src/assets/js9-to-be-compiled/js9.js",
              "src/assets/js9-to-be-compiled/js9plugins.js"
            ],
            "server": "src/main.server.ts",
            "outputMode": "server",
            "ssr": {
              "entry": "src/server.ts"
            }
          },
          "configurations": {
            "prod-deployment": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "5MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all",
              "outputPath": "dist/catch-frontend-ng20",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.local.ts",
                  "with": "src/environments/environment.prod-deployment.ts"
                }
              ]
            },
            "dev-deployment": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "5MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all",
              "outputPath": "dist/catch-frontend-ng20",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.local.ts",
                  "with": "src/environments/environment.dev-deployment.ts"
                }
              ]
            },
            "local": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "outputPath": "dist/catch-frontend-ng20"
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "options": {
            "proxyConfig": "proxy.conf.json"
          },
          "configurations": {
            "prod-deployment": {
              "buildTarget": "catch-frontend-ng20:build:prod-deployment"
            },
            "dev-deployment": {
              "buildTarget": "catch-frontend-ng20:build:dev-deployment"
            },
            "local": {
              "buildTarget": "catch-frontend-ng20:build:local"
            }
          },
          "defaultConfiguration": "local"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "styles": ["src/styles/styles.scss"]
          }
        }
      }
    }
  }
}
